{
  "name": "webpack-typescript-threejs",
  "version": "1.0.1",
  "description": "compile SPA for game seed. Support typescript.",
  "main": "index.js",
  "scripts": {
    "start": "npm run build:live",
    "build:live": "nodemon --exec ./node_modules/.bin/ts-node -- ./index.ts",
    "prepublish": "typings install",
    "watch": "cross-env NODE_ENV=development webpack --watch --config webpack.config.js ",
    "live": "cross-env NODE_ENV=development webpack-dev-server --hot --inline",
    "server": "cross-env NODE_ENV=development webpack --config webpack.config.js --progress --colors --display-error-details",
    "server-watch": "cross-env NODE_ENV=development webpack --config webpack.config.js --progress --colors --watch --display-error-details",
    "server-dev": "cross-env NODE_ENV=development webpack-dev-server --progress --colors --display-error-details",
    "server-dev-watch": "cross-env NODE_ENV=development webpack-dev-server --progress --colors --watch --display-error-details",
    "build-dev": "cross-env NODE_ENV=development webpack-dev-server --progress",
    "build-server": "cross-env NODE_ENV=development webpack --progress --colors",
    "build-production": "cross-env NODE_ENV=production webpack --progress",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Allen Wahlberg",
  "license": "MIT",
  "devDependencies": {
    "@types/es6-shim": "^0.31.32",
    "@types/node": "^6.0.63",
    "@types/three": "^0.83.1",
    "babel-cli": "^6.23.0",
    "babel-core": "^6.0.20",
    "babel-loader": "^6.3.2",
    "babel-preset-env": "^1.1.8",
    "babel-preset-es2015": "^6.0.15",
    "babel-preset-react": "^6.0.15",
    "babel-preset-stage-0": "^6.0.15",
    "bourbon": "^4.2.7",
    "cross-env": "^3.1.4",
    "css-loader": "^0.26.1",
    "es6-shim": "^0.35.1",
    "exports-loader": "^0.6.3",
    "extract-text-webpack-plugin": "^1.0.1",
    "html-loader": "^0.4.4",
    "html-webpack-plugin": "^2.26.0",
    "html-webpack-template": "^6.0.0",
    "image-webpack-loader": "^3.1.0",
    "imports-loader": "^0.6.5",
    "json-loader": "^0.5.4",
    "localforage": "^1.4.3",
    "lodash": "^4.17.4",
    "manifest-revision-webpack-plugin": "^0.3.0",
    "node-sass": "^4.5.0",
    "path": "^0.12.7",
    "raw-loader": "^0.5.1",
    "sass-loader": "^4.1.1",
    "style-loader": "^0.13.1",
    "ts-loader": "^1.3.3",
    "typescript": "^2.3.0-dev.20170218",
    "typings": "^2.1.0",
    "webgl-obj-loader": "^0.1.1",
    "webpack": "^1.14.0",
    "webpack-dev-middleware": "^1.2.0",
    "webpack-dev-server": "^1.16.2",
    "webpack-hot-middleware": "^2.0.0",
    "webpack-typescript": "^0.5.6"
  },
  "dependencies": {
    "three": "^0.83.0",
    "underscore": "^1.8.3"
  }
}
